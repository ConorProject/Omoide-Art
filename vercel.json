{
  "functions": {
    "api/generate.js": {
      "maxDuration": 30
    },
    "api/webhook-generate.js": {
      "maxDuration": 300
    },
    "api/gallery-update.js": {
      "maxDuration": 30
    },
    "api/gallery.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/gallery/(.*)",
      "destination": "/gallery/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-cleanup",
      "schedule": "0 2 * * *"
    }
  ]
}